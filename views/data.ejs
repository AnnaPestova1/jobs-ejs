   <%- include("partials/head.ejs") %> 
<%- include("partials/header.ejs") %>
   <h2>Sky Events List</h2>
    <table id="data-table">
      <tr id="data-table-header">
        <th>Event</th>
        <th>Name</th>
        <th>When</th>
        <th>Description</th>
        <th colspan="2"></th>
      </tr>
      <% if (data && data.length) { %>
        <% data.forEach((d) => { %>
          <tr>
            <td><%= d.event %></td>
            <td><%= d.name %></td>
             <td><%= d.date %></td>
            <td><%= d.description %></td>
            <td>
                <a href="/data/new">
                <button type="button">create new data</button>
                </a>
            </td>
            <td>
                <a href="/data/edit/<%= data.id %>">
                <button type="button">edit</button>
                </a>
            </td>
            <td>
                <form method="POST" action="/data/delete/<%= data.id %>" style="display: inline">
                <button type="button">delete</button>
                <input type="hidden" name="_csrf" value="<%= _csrf %>" />
                </form>
            </td>
          </tr>
        <% }) %>
      <% } %>
    </table>
    <%- include("partials/footer.ejs") %>